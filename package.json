{
  "name": "@epsilondelta-ai/frontend-utils",
  "version": "0.0.4",
  "description": "Frontend utilities for EpsilonDelta",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Epsilondelta-ai/frontend-utils.git"
  },
  "source": "index.ts",
  "main": "index.js",
  "module": "index.mjs",
  "types": "index.d.ts",
  "targets": {
    "main": {
      "isLibrary": true,
      "optimize": true
    },
    "module": {
      "isLibrary": true,
      "outputFormat": "esmodule",
      "optimize": true
    }
  },
  "exports": {
    ".": {
      "require": "./index.js",
      "import": "./index.mjs",
      "types": "./index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "test": "bun test",
    "build": "parcel build"
  },
  "devDependencies": {
    "@parcel/packager-ts": "^2.16.0",
    "@parcel/transformer-typescript-types": "^2.16.0",
    "@types/bun": "latest",
    "parcel": "^2.16.0"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "firebase": "^12.3.0"
  }
}
